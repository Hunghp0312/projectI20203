package edu.week1;
import java.util.Scanner;
public class BaiTap8 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		for(int i = 2; i*i <=n;i++) {
			if(n%i == 0) {
				System.out.println(i);
				break;
			}
		}
		System.out.println("Hoang Phuc Hung - 20183756");
	}
}
